{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Resource": [
            ],
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ]
        },
        {
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::codepipeline-us-east-1-*",
                "arn:aws:s3:::codepipeline-us-east-1-*/*"
            ],
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:GetObjectVersion"
            ]
        },
        {
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::capsule.modus.app",
                "arn:aws:s3:::capsule.modus.app/*"
            ],
            "Action": [
                "s3:PutObject",
                "s3:Get*",
                "s3:List*"
            ]
        }
    ]
}
